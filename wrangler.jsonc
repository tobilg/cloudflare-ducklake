{
  "name": "cloudflare-ducklake",
  "compatibility_date": "2025-06-06",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "main": "src/index.ts",
  "preview_urls": false,
  "migrations": [
    {
      "new_sqlite_classes": ["Container"],
      "tag": "v1"
    }
  ],
  "containers": [
    {
      "name": "cloudflare-ducklake",
      "image": "./Dockerfile",
      "class_name": "DuckDBContainer",
      "max_instances": 2,
      "configuration": {
        "memory": "4GB",
        "vcpu": 2
      }
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "DuckDBContainer",
        "name": "CONTAINER",
        "script_name": "src/index.ts"
      }
    ]
  },
  "observability": {
    "enabled": true
  }
}
